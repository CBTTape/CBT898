/* REXX */
    ispclib="'LSE.SBLSCLI0.VB'"             /* DuPont                 */
    ispclib="'SYS1.SBLSCLI0'"               /* ETS                    */
    Address TSO "ALTLIB ACTIVATE APPLICATION(CLIST) DA("ispclib")"
    Address ISPEXEC
    "LIBDEF ISPPLIB DATASET ID('SYS1.SBLSPNL0')"
    "LIBDEF ISPSLIB DATASET ID('SYS1.SBLSKEL0')"
    "LIBDEF ISPTLIB DATASET ID('SYS1.SBLSTBL0')"
    "LIBDEF ISPMLIB DATASET ID('SYS1.SBLSMSG0')"
    "SELECT PGM(BLSGLIBD)",
        "PARM(PANEL(BLSPPRIM)) NEWAPPL(BLSG) PASSLIB"
    Address TSO "ALTLIB DEACTIVATE APPLICATION(CLIST)"
    "LIBDEF ISPPLIB"
    "LIBDEF ISPSLIB"
    "LIBDEF ISPTLIB"
    "LIBDEF ISPMLIB"
