)BODY EXPAND(\\) LMSG(LMSGID1) SMSG(LMSGID1)
~&CSAHEAD1
~&SID#====>¬ZCMD                                              ~Scroll#===>`AMT $
?LMSGID1
¢MAP1 -------------------------------------------------------------------------¢
)INIT
 VGET ZSYSID
 &SID = &ZSYSID
 .HELP   = CSAMP100
 IF (&AMT = &Z) &AMT = 'CSR'
 &AMT = TRANS(TRUNC(&AMT,1) M,MAX H,HALF P,PAGE C,CSR D,DATA MSG=ISPG024)
 IF (&CSAX01 = &Z) &CSAX01 = 'YELLOW'
 IF (&CSAX02 = &Z) &CSAX02 = 'WHITE'
 IF (&CSAX03 = &Z) &CSAX03 = 'RED'
 IF (&CSAX04 = &Z) &CSAX04 = 'YELLOW'
 IF (&CSAX11 = &Z) &CSAX11 = 'YELLOW'
 IF (&CSAX12 = &Z) &CSAX12 = 'GREEN'
 IF (&CSAX13 = &Z) &CSAX13 = 'PINK'
 IF (&CSAX14 = &Z) &CSAX13 = 'BLUE'
 IF (&CSAX15 = &Z) &CSAX15 = 'RED'
 IF (&CSAX16 = &Z) &CSAX16 = 'TURQ'
 IF (&CSAX17 = &Z) &CSAX17 = 'WHITE'
 IF (&CSAX18 = &Z) &CSAX18 = 'YELLOW'
)PROC
 &AMT = TRANS(TRUNC(&AMT,1) M,MAX H,HALF P,PAGE C,CSR D,DATA MSG=ISPG024)
