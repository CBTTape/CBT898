//C838704D JOB (TPCICS001),'Distribute CSGCH*',MSGLEVEL=1,
//        MSGCLASS=X,CLASS=A,NOTIFY=&SYSUID TYPRUN=SCAN
//*
//VARS SET DSN=TPCICS.LOGON
//*
//*-------------------------------------------------------------------*
//* Watch the copying of member CSGCH$ accross systems as the TELSTA  *
//* prototype may cream carefully sculped member on remote systems.   *
//*-------------------------------------------------------------------*
//*
//COPY1    EXEC PGM=IEBCOPY,PARM='SIZE=200K'
//*
//IN1      DD  DISP=SHR,DSN=TPCICS.C838704.$$
//*
//EXEC     DD  DISP=SHR,DSN=&DSN..EXEC
//PANEL    DD  DISP=SHR,DSN=&DSN..ISPPLIB
//MSG      DD  DISP=SHR,DSN=&DSN..ISPMLIB
//SKELS    DD  DISP=SHR,DSN=&DSN..ISPSLIB
//*
//SYSUT3   DD  UNIT=VIO,SPACE=(80,(6000,2000))
//SYSUT4   DD  UNIT=VIO,SPACE=(80,(6000,2000))
//SYSPRINT DD  SYSOUT=*
//SYSIN    DD  *
 COPY INDD=((IN1,R)),OUTDD=EXEC
  SELECT MEMBER=CSGCH
  SELECT MEMBER=CSGCH$
  SELECT MEMBER=CSGCH$$
  SELECT MEMBER=CSGCH$A1
  SELECT MEMBER=CSGCH$IX
  SELECT MEMBER=CSGCH$RM
  SELECT MEMBER=CSGCH$VF
  SELECT MEMBER=CSGCHELP
  SELECT MEMBER=CSGCHJ
  SELECT MEMBER=CSGCHJ1
  SELECT MEMBER=CSGCHSLV
  SELECT MEMBER=CSGCHSRC
  SELECT MEMBER=CSGCHXCU
  SELECT MEMBER=CSGCHXJC
  SELECT MEMBER=CSGCHXOC
  SELECT MEMBER=CSGCHXTP
  SELECT MEMBER=CSGCHXWO
  SELECT MEMBER=HELPDRVR
  SELECT MEMBER=HELPTT
  SELECT MEMBER=HELPQR
  SELECT MEMBER=HELPTN
  SELECT MEMBER=CSGCHTRG
 COPY INDD=((IN1,R)),OUTDD=EXEC
  SELECT MEMBER=((CSGCH$RM,$$README))
 COPY INDD=((IN1,R)),OUTDD=PANEL
  SELECT MEMBER=CSGCHP
  SELECT MEMBER=CSGCHPCO
  SELECT MEMBER=CSGCHPDY
  SELECT MEMBER=CSGCHPH
  SELECT MEMBER=CSGCHPLG
  SELECT MEMBER=CSGCHPOP
  SELECT MEMBER=CSGCHPR
  SELECT MEMBER=CSGCHTRP
/*
