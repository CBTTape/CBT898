 COPY INDD=((IN1,R)),OUTDD=EXEC
  SELECT MEMBER=$WHOAMI
  SELECT MEMBER=BR
  SELECT MEMBER=$CSTRING
  SELECT MEMBER=$DATEMAN
  SELECT MEMBER=REAL
  SELECT MEMBER=WORDAT
 COPY INDD=((IN1,R)),OUTDD=PANEL
  SELECT MEMBER=HELPDRVP
  SELECT MEMBER=HELPTBD1
 COPY INDD=((IN1,R)),OUTDD=SKELS
  SELECT MEMBER=HELPDRVS
 COPY INDD=((IN1,R)),OUTDD=MSG
  SELECT MEMBER=GC00
