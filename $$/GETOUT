/* REXX */
trace r
    Address ISPEXEC
    "CONTROL ERRORS CANCEL"
    Do i=1 to 5
/*      x=com('END')                                                  */
        xx=('SWAP;=X')
        x=com(xx)
Say rb
    End
    Exit

com:Arg comd;"CONTROL NONDISPL ENTER"
             "DISPLAY COMMAND(comd) RETBUFFR(rb)";Return rc
