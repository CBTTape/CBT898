)ATTR
 _ TYPE(INPUT) INTENS(HIGH) CAPS(ON)
 @ TYPE(&SELCTDAT) INTENS(HIGH) COLOR(YELLOW)
 { TYPE(TEXT) INTENS(HIGH) COLOR(YELLOW)
 # TYPE(OUTPUT) SKIP(ON) INTENS(LOW) CAPS(ON)
 ¬ TYPE(OUTPUT) SKIP(ON) INTENS(LOW) CAPS(ON) JUST(RIGHT)
)BODY EXPAND(\\)
{Member Selection - &DSN -\-\-
%&SID%===>_ZCMD                                            %Scroll ===>_AMT +
+    Name              VV.MM  Created   Last modified  Size   Init   Mod   Id
)MODEL
_Z #MEMBER  @Z        #Z     #Z        #Z       #Z    ¬Z     ¬Z     ¬Z    #Z
)INIT
  &SID   = &ZSYSID
  &VARS1 = '(SEL SELCTD VERSMOD ZLCDATE ZLMDATE'
  &VARS2 = 'ZLMTIME ZLCNORC ZLINORC ZLMNORC ZLUSER)'
  .ZVARS = '&VARS1 &VARS2'
  &XAMT = TRANS(TRUNC(&AMT,1) H,HALF P,PAGE C,CSR D,DATA *,*)
  IF (&XAMT = HALF,PAGE,CSR,DATA) &AMT = &XAMT
  IF (&SELCTDAT ¬= INPUT)
     &SELCTDAT = OUTPUT
)PROC
  &XAMT = TRANS(TRUNC(&AMT,1) H,HALF P,PAGE C,CSR D,DATA *,*)
  IF (&XAMT = HALF,PAGE,CSR,DATA) &AMT = &XAMT
)END
