)ATTR DEFAULT(%$_)
~  TYPE(TEXT)    INTENS(HIGH) COLOR(&STCX10)
#  TYPE(TEXT)    INTENS(HIGH) COLOR(&STCX09)
?  TYPE(OUTPUT)  INTENS(HIGH) COLOR(&STCX09)
`  TYPE(INPUT)   INTENS(HIGH) COLOR(&STCX08)
¬  TYPE(INPUT)   INTENS(HIGH) CAPS(OFF)
¢  AREA(DYNAMIC) SCROLL(ON)   EXTEND(ON) USERMOD(AA)
!  AREA(DYNAMIC)
05 TYPE(DATAOUT) INTENS(HIGH) COLOR(&STCX10) HILITE(USCORE)
11 TYPE(DATAIN)  INTENS(LOW)  COLOR(&STCX11)
12 TYPE(DATAOUT) INTENS(HIGH) COLOR(&STCX12)
13 TYPE(DATAOUT) INTENS(LOW)  COLOR(&STCX13)
14 TYPE(DATAOUT) INTENS(HIGH) COLOR(&STCX14)
15 TYPE(DATAOUT) INTENS(LOW)  COLOR(&STCX15)
16 TYPE(DATAIN)  INTENS(HIGH) COLOR(&STCX16)
17 TYPE(DATAIN)  INTENS(HIGH) COLOR(&STCX17)
18 TYPE(DATAOUT) INTENS(LOW)  COLOR(&STCX11)
19 TYPE(DATAOUT) INTENS(HIGH) COLOR(&STCX19)
1A TYPE(DATAOUT) INTENS(HIGH) COLOR(PINK)
1B TYPE(DATAOUT) INTENS(HIGH) COLOR(YELLOW)
 w TYPE(CHAR) COLOR(WHITE)
 y TYPE(CHAR) COLOR(YELLOW)
 b TYPE(CHAR) COLOR(BLUE)
 B TYPE(CHAR) COLOR(BLUE) HILITE(USCORE)
 r TYPE(CHAR) COLOR(RED)
 t TYPE(CHAR) COLOR(TURQ)
 T TYPE(CHAR) COLOR(TURQ) HILITE(USCORE)
 g TYPE(CHAR) COLOR(GREEN)
)BODY EXPAND(\\) LMSG(XX2XX)
~&HEADING           ~---------------------------------------- &scroll
~&SID#====>¬ZCMD                                            ?Z~Scroll#===>`AMT $
!HEADSUB,HEADSHD                                                               !
¢DATA -------------------------------------------------------------------------¢
)INIT
 VGET ZSYSID
 &SID = &ZSYSID
 .ZVARS  = '(XX2XX)'
 &XX2XX = ''
 .CURSOR = ZCMD
 IF (&SHOWPHLP = &Z)
     &SHOWPHLP = DASMP100

 IF (&AMT = &Z) &AMT = CSR
 &AMT = TRANS(TRUNC(&AMT,1) M,MAX H,HALF P,PAGE C,CSR D,DATA MSG=ISPG024)
 IF (&STCX10 = &Z) &STCX10 = 'YELLOW'
 IF (&STCX11 = &Z) &STCX11 = 'GREEN'
 IF (&STCX12 = &Z) &STCX12 = 'PINK'
 IF (&STCX13 = &Z) &STCX13 = 'BLUE'
 IF (&STCX14 = &Z) &STCX14 = 'YELLOW'
 IF (&STCX15 = &Z) &STCX15 = 'RED'
 IF (&STCX16 = &Z) &STCX16 = 'TURQ'
 IF (&STCX17 = &Z) &STCX17 = 'WHITE'
 IF (&STCX19 = &Z) &STCX19 = 'YELLOW'
)PROC
 &AMT = TRANS(TRUNC(&AMT,1) M,MAX H,HALF P,PAGE C,CSR D,DATA MSG=ISPG024)
 &NL = LVLINE(DATA)
 IF (&ZCMD = 'CAN','CANCEL','RES','RESET')
    .RESP = END
 &CURSOR = .CURSOR
 &CSRPOS = .CSRPOS
 &RESPON = .RESP
)END
