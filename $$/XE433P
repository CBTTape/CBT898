)INIT
/* If (&TIME = &Z) &TIME = &ZTIME                                     */
 IF (&XEA125 ¬= &Z) &C03 = '<'
 IF (&XEA1  ¬= &Z)
   IF (&C3 ¬= &Z) &C03 = '&C03.>'
   ELSE           &C03 = '>'
 &C3 = &C03
 &SID  = &ZSYSID
)REINIT
/* &TIME = &ZTIME                                                     */
)PROC
  &#ZCMD# = &ZCMD
  REFRESH(ZCMD)
  &FLDCSR = .CURSOR
)END
