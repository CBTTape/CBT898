)INIT
/* If (&TIME = &Z) &TIME = &ZTIME                                     */
 IF (&XEA151 ¬= &Z) &C04 = '<'
 IF (&XEA1  ¬= &Z)
   IF (&C4 ¬= &Z) &C04 = '&C04.>'
   ELSE           &C04 = '>'
 &C4 = &C04
 &SID  = &ZSYSID
)REINIT
/* &TIME = &ZTIME                                                     */
)PROC
  &#ZCMD# = &ZCMD
  REFRESH(ZCMD)
  &FLDCSR = .CURSOR
)END
