*
* THE L.B.D. U BYUTE UNPATTENTED SYS.
*
DMP130    WCCDC W,,COLOR
          SBADC 04,01,PHN,Y,O
          DC   CL32'Miscellaneous commands (cont''d):'
          RADC  04,33,PHN,Y,O,05,01,' ','CONT.D FROM PREV "SBA" DETS'
          RADC  05,01,PLN,B,O,06,01,' '
          SBADC 06,01,PHN,Y,O
          DC   CL08'  F str '
          SBADC 06,10,PLN,B,O
          DC   CL52': Find string. Searches the next 4096 characters foX
               r'
          SBADC 06,63,PHN,Y,O
          DC   CL03'str'
          SBADC 06,67,PLN,B,O
          DC   CL01'.'
          RADC  06,68,PLN,B,O,07,01,' ','CONT.D FROM PREV "SBA" DETS'
          RADC  07,01,PLN,B,O,07,13,' '
          SBADC 07,12,PLN,B,O,'CONT.D FROM PREV "SBA" DETS'
          DC   CL05'Where'
          SBADC 07,18,PHN,Y,O
          DC   CL03'str'
          SBADC 07,22,PLN,B,O
          DC   CL30'can have the following values:'
          RADC  07,52,PLN,B,O,08,01,' ','CONT.D FROM PREV "SBA" DETS'
          RADC  08,01,PHN,Y,O,08,15,' '
          SBADC 08,14,PHN,Y,O,'CONT.D FROM PREV "SBA" DETS'
          DC   CL17'characters string'
          SBADC 08,32,PLN,B,O
          DC   CL40'imbedded spaces permitted (max 40 chars)'
          RADC  08,72,PLN,B,O,09,01,' ','CONT.D FROM PREV "SBA" DETS'
          RADC  09,01,PHN,Y,O,09,15,' '
          SBADC 09,14,PHN,Y,O,'CONT.D FROM PREV "SBA" DETS'
          DC   CL20'LC''lower.case.string'
          SBADC 09,35,PLN,B,O
          DC   CL35'will not be converted to upper case'
          RADC  09,70,PLN,B,O,10,01,' ','CONT.D FROM PREV "SBA" DETS'
          RADC  10,01,PHN,Y,O,10,15,' '
          SBADC 10,14,PHN,Y,O,'CONT.D FROM PREV "SBA" DETS'
          DC   CL16'X''hex.characters'
          SBADC 10,31,PLN,B,O
          DC   CL19'look for hex string'
          RADC  10,50,PLN,B,O,11,01,' ','CONT.D FROM PREV "SBA" DETS'
          RADC  11,01,PLN,B,O,11,13,' '
          SBADC 11,12,PLN,B,O,'CONT.D FROM PREV "SBA" DETS'
          DC   CL68'NOTE - Under partial displays, (re)finds are only aX
               ttempted for the '
          RADC  12,01,PLN,B,O,12,20,' '
          SBADC 12,19,PLN,B,O,'CONT.D FROM PREV "SBA" DETS'
          DC   CL15'display length.'
          RADC  12,34,PLN,B,O,13,01,' ','CONT.D FROM PREV "SBA" DETS'
          RADC  13,01,PLN,B,O,14,01,' '
          SBADC 14,01,PHN,Y,O
          DC   CL08'  FN    '
          SBADC 14,10,PLN,B,O
          DC   CL31': Search for previously entered'
          SBADC 14,42,PHN,Y,O
          DC   CL03'str'
          SBADC 14,46,PLN,B,O
          DC   CL34'in the next lot of 4096 bytes.    '
          RADC  15,01,PLN,B,O,16,01,' '
          SBADC 16,01,PHN,Y,O
          DC   CL08'  FP    '
          SBADC 16,10,PLN,B,O
          DC   CL31': Search for previously entered'
          SBADC 16,42,PHN,Y,O
          DC   CL03'str'
          SBADC 16,46,PLN,B,O
          DC   CL34'in the previous lot of 4096 bytes.'
          RADC  17,01,PLN,B,O,18,01,' '
          SBADC 18,01,PHN,Y,O
          DC   CL08'  REFIND'
          SBADC 18,10,PLN,B,O
          DC   CL63': Refind the previosly entered string (PF5 defaultsX
                to refind).'
          RADC  18,73,PLN,B,O,19,01,' ','CONT.D FROM PREV "SBA" DETS'
          RADC  19,01,PLN,B,O,19,13,' '
          SBADC 19,12,PLN,B,O,'CONT.D FROM PREV "SBA" DETS'
          DC   CL68'If the end of the 4096 bytes is signalled, the nextX
                refind will     '
          RADC  20,01,PLN,B,O,20,13,' '
          SBADC 20,12,PLN,B,O,'CONT.D FROM PREV "SBA" DETS'
          DC   CL29'start from the top of screen.'
          RADC  20,41,PLN,B,O,21,01,' ','CONT.D FROM PREV "SBA" DETS'
          RADC  21,01,PLN,B,O,22,01,' '
          SBADC 22,01,PHN,Y,O
          DC   CL08'  ?F    '
          SBADC 22,10,PLN,B,O
          DC   CL50': Redisplay last find command on the command line.'
          RADC  22,60,PLN,B,O,23,01,' ','CONT.D FROM PREV "SBA" DETS'
          RADC  23,01,PLN,B,O,24,01,' '
          RADC  24,01,PLN,B,O,24,80,' '
DMPSZ130 EQU   *-DMP130
