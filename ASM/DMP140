DMP140    WCCDC W,,COLOR
          SBADC 04,01,PHN,Y,O
          DC   CL32'Miscellaneous commands (cont''d):'
          RADC  04,33,PHN,Y,O,05,01,' ','CONT.D FROM PREV "SBA" DETS'
          RADC  05,01,PLN,B,O,06,01,' '
          SBADC 06,01,PHN,Y,O
          DC   CL16'  FINDLIM #pages'
          RADC  06,17,PHN,Y,O,07,01,' ','CONT.D FROM PREV "SBA" DETS'
          RADC  07,01,PLN,B,O,07,11,' '
          SBADC 07,10,PLN,B,O,'CONT.D FROM PREV "SBA" DETS'
          DC   CL50': Resets the number of pages for the search limit.'
          RADC  07,60,PLN,B,O,08,01,' ','CONT.D FROM PREV "SBA" DETS'
          RADC  08,01,PLN,B,O,08,13,' '
          SBADC 08,12,PLN,B,O,'CONT.D FROM PREV "SBA" DETS'
          DC   CL03'The'
          SBADC 08,16,PHN,Y,O
          DC   CL06'#pages'
          SBADC 08,23,PLN,B,O
          DC   CL50'must be non-blank and not greater than 1000 pages.'
          RADC  08,73,PLN,B,O,09,01,' ','CONT.D FROM PREV "SBA" DETS'
          RADC  09,01,PLN,B,O,09,13,' '
          SBADC 09,12,PLN,B,O,'CONT.D FROM PREV "SBA" DETS'
          DC   CL56'1000 pages will set the search limit to 4,096,000 bX
               ytes.'
          RADC  09,68,PLN,B,O,10,01,' ','CONT.D FROM PREV "SBA" DETS'
          RADC  10,01,PLN,B,O,11,01,' '
          SBADC 11,01,PHN,Y,O
          DC   CL06'  ISPF'
          RADC  11,07,PHN,Y,O,12,01,' ','CONT.D FROM PREV "SBA" DETS'
          RADC  12,01,PLN,B,O,12,11,' '
          SBADC 12,10,PLN,B,O,'CONT.D FROM PREV "SBA" DETS'
          DC   CL70': Invoke ISPF primary options menu to permit ISPF oX
               perations and split'
          RADC  13,01,PLN,B,O,13,13,' '
          SBADC 13,12,PLN,B,O,'CONT.D FROM PREV "SBA" DETS'
          DC   CL12'screen mode.'
          RADC  13,24,PLN,B,O,14,01,' ','CONT.D FROM PREV "SBA" DETS'
          RADC  14,01,PLN,B,O,15,01,' '
          SBADC 15,01,PHN,Y,O
          DC   CL05'  MSG'
          RADC  15,06,PHN,Y,O,16,01,' ','CONT.D FROM PREV "SBA" DETS'
          RADC  16,01,PLN,B,O,16,11,' '
          SBADC 16,10,PLN,B,O,'CONT.D FROM PREV "SBA" DETS'
          DC   CL49': Redisplay the initial entry message if present.'
          RADC  16,59,PLN,B,O,17,01,' ','CONT.D FROM PREV "SBA" DETS'
          RADC  17,01,PLN,B,O,18,01,' '
          SBADC 18,01,PHN,Y,O
          DC   CL06'  QUIT'
          RADC  18,07,PHN,Y,O,19,01,' ','CONT.D FROM PREV "SBA" DETS'
          RADC  19,01,PLN,B,O,19,11,' '
          SBADC 19,10,PLN,B,O,'CONT.D FROM PREV "SBA" DETS'
          DC   CL06': Exit'
          SBADC 19,17,PHN,Y,O
          DC   CL08'PROGDUMP'
          SBADC 19,26,PLN,B,O
          DC   CL24'with a return code of 8.'
          RADC  19,50,PLN,B,O,20,01,' ','CONT.D FROM PREV "SBA" DETS'
          RADC  20,01,PLN,B,O,21,01,' '
          SBADC 21,01,PHN,Y,O
          DC   CL04'  R0'
          SBADC 21,06,PLN,B,O
          DC   CL02'to'
          SBADC 21,09,PHN,Y,O
          DC   CL03'R15'
          RADC  21,12,PHN,Y,O,22,01,' ','CONT.D FROM PREV "SBA" DETS'
          RADC  22,01,PLN,B,O,22,11,' '
          SBADC 22,10,PLN,B,O,'CONT.D FROM PREV "SBA" DETS'
          DC   CL50': Display the address pointed to by that register.'
          RADC  22,60,PLN,B,O,23,01,' ','CONT.D FROM PREV "SBA" DETS'
          RADC  23,01,PLN,B,O,24,01,' '
          RADC  24,01,PLN,B,O,24,31,' '
          SBADC 24,30,PLN,B,O,'CONT.D FROM PREV "SBA" DETS'
          DC   CL24'(Continued next screen.)'
          RADC  24,54,PLN,B,O,01,01,' ','CONT.D FROM PREV "SBA" DETS'
DMPSZ140 EQU   *-DMP140
