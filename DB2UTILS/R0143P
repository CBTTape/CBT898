)ATTR DEFAULT(%+_)
    _ TYPE(INPUT) INTENS(HIGH) COLOR(RED)    HILITE(USCORE)
    @ TYPE(TEXT)  INTENS(HIGH) COLOR(YELLOW)
    $ TYPE(OUTPUT) INTENS(HIGH) COLOR(WHITE) JUST(RIGHT)
    ? TYPE(OUTPUT) INTENS(LOW) COLOR(BLUE)
    ` TYPE(OUTPUT) INTENS(NON)
)BODY EXPAND(\\) WINDOW(25,20) LMSG(MYLMSG) SMSG(MYLMSG)

_Z $T1 ?EDTT1   `z+
%  $T2 ?EDTT2
%  $T3 ?EDTT3
%  $T4 ?EDTT4
%  $T5 ?EDTT5
%  $T6 ?EDTT6
%  $T7 ?EDTT7
%  $T8 ?EDTT8
%  $T9 ?EDTT9
%  $T10?EDTT10
%  $T11?EDTT11
%  $T12?EDTT12
%  $T13?EDTT13
%  $T14?EDTT14
%  $T15?EDTT15
%  $T16?EDTT16
+Select applicable DDN
+from list above or@END
+PF key to abandon.
)INIT
  VGET ZSYSID ASIS
  &SID = &ZSYSID
  .ZVARS = '(TT,MYLMSG)'
  &GC001 = 'Valid values are 1 through &EDTOCR..'
)PROC
  VER(&TT,NB,RANGE,1,&EDTOCR,MSG=GC001)
)END
