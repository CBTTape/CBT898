         MACRO                                                 @BBDA20X 00100000
         DFHCSAD  &TYPE=DSECT,                                         *00200000
               &REG=13,            REGISTER FOR TYPE=LOCATE REQUEST    *00250000
               &SSP=0,             SNAPSHOT PROGRAM ADDRESS            *00600000
               &OPFL=0,            OPT. FEAT. LIST ADDRESS             *00700000
               &MXTDS=0,           CLASS MAX TASK FIELDS ADDRESS       *00750000
               &CSA=0,             CSA ADDRESS                         *00800000
               &ABEND=0,           ABNORMAL TERMINATION ROUTINE ADDRESS*01300000
               &SRA=0,             SRA ADDRESS                         *01400000
               &PCA=0,             PCA ADDRESS                         *01500000
               &LFN=0,             LIFO ENTER ADDRESS                  *01530000
               &LFX=0,             LIFO EXIT ADDRESS                   *01570000
               &SECBL=0,           SECURITY CLASS BLOCK                *01580000
               &USERKEY=,          STORAGE PROTECTION USER KEY         *01590000
               &OLDKEYR=0,         Register for original exec key      *01600000
               &KEYR=2,            Register to be used for key switch  *01610000
               &AFCBPTR=,          (input) AFCB address to speed LOCATE*01615000
               &DUMMY=                                      @01281 @OUC 01620000
.********************************************************************** 01630000
.*                                                                    * 01640000
.* MACRO NAME = DFHCSAD                                               * 01650000
.*                                                                    * 01660000
.* DESCRIPTIVE NAME = CICS        COMMON SYSTEM AREA GENERATOR        * 01670000
.*                                                                    * 01680000
.*   @BANNER_START                                                    * 01681400
.*   Licensed Materials - Property of IBM                             * 01682800
.*                                                                    * 01684200
.*   "Restricted Materials of IBM"                                    * 01685600
.*                                                                    * 01687000
