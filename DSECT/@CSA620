.*                                                                    * 01700000
.* STATUS = 6.2.0                                                     * 01710000
.*                                                                    * 01720000
.* FUNCTION = DFHCSAD GENERATES THE DSECT WHICH IS USED BY CICS/VS    * 01730000
.*            TO REFERENCE ITS GLOBAL CONTROL BLOCK, THE COMMON       * 01740000
.*            SYSTEM AREA.  IT IS ALSO USED BY THE MODULE DFHCSA TO   * 01750000
.*            RESERVE STORAGE FOR THE COMMON SYSTEM AREA.             * 01760000
.*                                                                    * 01770000
.* NOTES :                                                            * 01780000
.*  DEPENDENCIES = S/370                                              * 01790000
.*  RESTRICTIONS = NONE                                               * 01800000
.*  REGISTER CONVENTIONS = NOT APPLICABLE                             * 01810000
.*  PATCH LABEL = NOT APPLICABLE                                      * 01820000
.*  MODULE TYPE = MACRO                                               * 01830000
.*  PROCESSOR = ASSEMBLER                                             * 01840000
.*  MODULE SIZE = NOT APPLICABLE                                      * 01850000
.*  ATTRIBUTES = NOT APPLICABLE                                       * 01860000
.*                                                                    * 01870000
.* MACROS :  DFHAFCD, DFHEJECT, DFHPRINT, DFHSYS                      * 01880000
.*                                                                    * 01890000
