         AIF   ('&AFCBPTR' EQ '').AFPTRBP  afcbptr not supplied    @LQA 04202000
         AIF   ('&AFCBPTR'(1,1) NE '(').AFPTRNR not register spec  @LQA 04203000
&AREG    SETC  '&AFCBPTR'(2,K'&AFCBPTR-2)  register value          @LQA 04204000
         AGO   .AFPTRST                                            @LQA 04205000
.AFPTRNR ANOP                                                      @LQA 04206000
         L     &AREG,&AFCBPTR      AFCB address into req'd register@LQA 04207000
         AGO   .AFPTRST                                            @LQA 04208000
.AFPTRBP ANOP                                                      @LQA 04209000
         DFHAFCD  TYPE=LOCATE,     FIND THE ADDRESS         @01267 @D2A*04210000
               REG=&AREG             OF THE AFCB            @01267 @LQC 04220000
.AFPTRST ANOP                                                      @LQA 04225000
         L     &REG,AFCSA-DFHAFCB(,&AREG) ADDRESS THE CSA   @01267 @LQC 04230000
*----------------------------------------------------------------*      04240000
         MEXIT                                              @01267 @D2A 04250000
.*                                                                    * 04260000
.CICSKEY ANOP  ,                                                   @NEA 04270000
.********************************************************************** 04280000
.* We are asked to switch into CICS execution key, and to return the  * 04290000
.* original execution key in OLDKEYR                                  * 04300000
.********************************************************************** 04310000
         AIF   ('&TYPE' NE 'CICSKEY').DSECT1                       @NEA 04320000
*----------------------------------------------------------------*      04330000
         IPK                       Get the current execution key   @NEA 04340000
*                                  in bits 24-27 of register 2     @NEA 04350000
         AIF   ('&OLDKEYR' EQ '2'                                      *04360000
               OR '&OLDKEYR' EQ 'R2').CICSKY1                      @NEA 04370000
         LR    &OLDKEYR,2          ... and save it to OLDKEYR      @NEA 04380000
.CICSKY1 ANOP                                                           04390000
&AREG    SETC  '&REG'              AFCB register default is &reg   @M7A 04391000
         AIF   ('&AFCBPTR' EQ '').AFPTRB1  afcbptr not supplied    @M7A 04392000
         AIF   ('&AFCBPTR'(1,1) NE '(').AFPTRN1 not register spec  @M7A 04393000
&AREG    SETC  '&AFCBPTR'(2,K'&AFCBPTR-2)  register value          @M7A 04394000
         AGO   .AFPTRS1                                            @M7A 04395000
.AFPTRN1 ANOP                                                      @M7A 04396000
         L     &AREG,&AFCBPTR      AFCB address into req'd register@M7A 04397000
         AGO   .AFPTRS1                                            @M7A 04398000
.AFPTRB1 ANOP                                                      @M7A 04399000
         DFHAFCD  TYPE=LOCATE,     Find the AFCB address               *04400000
               REG=&REG            using this register             @NEA 04410000
.AFPTRS1 ANOP                                                      @M7A 04415000
         L     &REG,AFCSA-DFHAFCB(,&AREG)  ADDRESS THE CSA         @M7C 04420000
         IC    &KEYR,CSACIKEY-DFHCSADS(,&REG)  Get the CICS key    @NEA 04430000
         SPKA  0(&KEYR)            Switch to CICS key              @NEA 04440000
*----------------------------------------------------------------*      04450000
         MEXIT  ,                                                  @NEA 04460000
.*                                                                      04470000
.********************************************************************** 04480000
.DSECT1  ANOP                                               @01267 @D2A 04490000
         AIF   ('&TYPE' NE 'DSECT').CSECT1                     @BBDA20X 04500000
         AIF   (&DFHCSAD).DSECTBY  NO DSECT IF ALREADY GENERATED       F04510000
                                                            @01268 @D5A 04520000
         DFHPRINT DSCT=SOMESTART                               @BBDB80J 04530000
*********************************************************************** 04540000
*            C I C S   C O M M O N   S Y S T E M   A R E A            * 04550000
*********************************************************************** 04560000
DFHCSADS DSECT             DUMMY SECTION - CSA                 @BB89999 04570000
.CSECT1  ANOP                                                  @BBDA20X 04580000
         USING *,13        USING REGISTER 13                   @BBDA20X 04590000
DFHCSABA EQU   *           COMMON SYSTEM AREA BEGIN ADDRESS    @BBDA20X 04600000
.*   DURING THE EARLY PART OF CICS/VS INITIALISATION A TEMPORARY      * 04610000
.*   CSA IS USED.  DFHSIP ALLOCATES AND CLEARS THE STORAGE FOR IT.    * 04620000
.*   IT IS USED BY DFHSIA1 AND BY DFHSIB1 UNTIL THE REAL CSA IS       * 04630000
.*   LOADED.   DFHSIB1 THEN COPIES MOST FIELDS FROM THE TEMPORARY     * 04640000
.*   CSA TO THE REAL ONE.  THE FIELDS WHICH ARE COPIED ARE            * 04650000
.*   GENERATED BY THIS MACRO USING 'DS' INSTRUCTIONS.  THOSE          * 04660000
.*   FIELDS WHICH ARE NOT COPIED AE GENERATED BY 'DC' INSTRUCTIONS.   * 04670000
*********************************************************************** 04680000
* * *                   S T O R A G E                             * * * 04700000
*********************************************************************** 04720000
